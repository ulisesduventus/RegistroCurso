                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_weight="1"
                                        android:hint="@string/apellido_materno"
                                        android:outlineAmbientShadowColor="@color/colorRojoDuventus"

                                        android:outlineSpotShadowColor="@color/clinicaType"
                                        app:boxBackgroundColor="#EEEEEE"
                                        app:boxCornerRadiusTopEnd="15dp"
                                        app:boxCornerRadiusTopStart="15dp"
                                        app:boxStrokeColor="@color/hintBlue"
                                        app:counterOverflowTextColor="@color/clinicaType"
                                        app:counterTextColor="@color/clinicaType"
                                        app:errorEnabled="true"
                                        app:hintTextAppearance="@style/TextHintLabel"

                                        app:hintTextColor="@color/hintBlue">

                                        <com.google.android.material.textfield.TextInputEditText
                                            android:id="@+id/txtApellidoMatET"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:digits="abcdefghijklmnñopqrstuvwxyzABCDEFGHIJKLMNÑOPQRSTUVWXYZáéíóúüÁÉÍÓÚ "
                                            android:inputType="textCapWords"
                                            android:maxLines="1"
                                            android:singleLine="true"
                                            android:textCursorDrawable="@drawable/custom_cursor" />

                                    </com.google.android.material.textfield.TextInputLayout>
                                </LinearLayout>


                                <LinearLayout
